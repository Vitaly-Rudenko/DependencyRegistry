{
    "$schema": "./node_modules/@stryker-mutator/core/schema/stryker-schema.json",
    "packageManager": "npm",
    "files": [
        "*",
        "lib/{,**}/*",
        "spec/{,**}/*"
    ],
    "mutate": [
        "lib/{,**}/*.js"
    ],
    "reporters": [
        "html",
        "clear-text",
        "progress"
    ],
    "testRunner": "jasmine",
    "coverageAnalysis": "perTest",
    "jasmineConfigFile": "spec/support/jasmine.json"
}